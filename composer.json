{
  "name": "macavv/routes",
  "description": "A laravel package to list your routes and filter through them.",
  "keywords": [
    "laravel",
    "macavv",
    "routes",
    "laravel-routes"
  ],
  "homepage": "https://github.com/macavv/routes",
  "license": "MIT",
  "authors": [
    {
      "name": "Jagadesha NH",
      "email": "opensource@macavv.com",
      "homepage": "http://macavv.com",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Macavv/routes"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "illuminate/support": "~5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "Macavv\\Routes\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Macavv\\Routes\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Macavv\\Routes\\RoutesServiceProvider"
      ]
    }
  }
}